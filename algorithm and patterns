     this dutch flag algorithm
     void sortColors(int* nums, int numsSize) {
    int low=0;
    int mid=0;
    int high=numsSize-1;
    while(mid<=high){
        if(nums[mid]==0){
            int temp=nums[low];
            nums[low]=nums[mid];
            nums[mid]=temp;
            mid++;
            low++;
        }else if(nums[mid]==1){
            mid++;
        }else{
         int temp=nums[high];
            nums[high]=nums[mid];
            nums[mid]=temp;
            high--; 
        }
    }
    for(int i=0;i<numsSize-1;i++){
        printf("%d",nums[i]);
    }
}
